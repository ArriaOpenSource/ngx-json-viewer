{
  "name": "ngx-json-viewer",
  "version": "3.0.2",
  "description": "JSON formatter / viewer for Angular 2/4/5/6/7/8+",
  "keywords": [
    "angular",
    "json"
  ],
  "author": "Vivo Xu <vivo@fancyber.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hivivo/ngx-json-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/hivivo/ngx-json-viewer/issues"
  },
  "homepage": "https://github.com/hivivo/ngx-json-viewer#readme",
  "main": "dist/index.ts",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "release": "yarn build && yarn publish ./dist",
    "start": "npm run build && npm --cwd ./example add ../dist && npm --cwd ./example start"
  },
  "peerDependencies": {
    "@angular/common": ">=10.0.0",
    "@angular/core": ">=10.0.0",
    "rxjs": ">=6.6.2"
  },
  "devDependencies": {
    "@angular/common": "^13.0.x",
    "@angular/compiler": "^13.0.x",
    "@angular/compiler-cli": "^13.0.x",
    "@angular/core": "^13.0.x",
    "ng-packagr": "^13.2.1",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "typescript": "~4.4.x",
    "zone.js": "^0.11.4"
  }
}
